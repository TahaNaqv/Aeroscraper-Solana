modules = ["nodejs-20", "rust-stable", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["solana-cli"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Setup Info"

[[workflows.workflow]]
name = "Setup Info"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash /home/runner/workspace/setup-info.sh"

[workflows.workflow.metadata]
outputType = "console"
